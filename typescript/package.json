{
  "name": "galaxi-backend",
  "version": "1.0.0",
  "description": "GOG Galaxy client backend for Galaxi",
  "main": "src/server.ts",
  "type": "module",
  "scripts": {
    "start": "bun src/server.ts",
    "dev": "bun --watch src/server.ts",
    "build": "bun build src/server.ts --compile --minify --target=bun-linux-x64 --outfile galaxi-backend"
  },
  "keywords": [
    "gog",
    "galaxy",
    "flutter"
  ],
  "author": "",
  "license": "GPL-3.0",
  "dependencies": {
    "axios": "^1.6.0",
    "md5": "^2.3.0"
  },
  "devDependencies": {
    "@types/md5": "^2.3.5",
    "@types/bun": "latest"
  }
}
